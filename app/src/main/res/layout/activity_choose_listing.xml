<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ChooseListing">

    <TextView
        android:id="@+id/textChooselisting"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Choose a listing:"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="117dp"
        tools:layout_editor_absoluteY="75dp" />

    <Button
        android:id="@+id/buttonSwim"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Swimming Pool"
        tools:layout_editor_absoluteX="125dp"
        tools:layout_editor_absoluteY="461dp" />

    <Button
        android:id="@+id/buttonWood"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Woodshop"
        tools:layout_editor_absoluteX="145dp"
        tools:layout_editor_absoluteY="288dp" />

    <Button
        android:id="@+id/buttonGameroom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Game Room"
        tools:layout_editor_absoluteX="142dp"
        tools:layout_editor_absoluteY="374dp" />

    <Button
        android:id="@+id/buttonGym"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Gym"
        tools:layout_editor_absoluteX="161dp"
        tools:layout_editor_absoluteY="205dp" />

</androidx.constraintlayout.widget.ConstraintLayout>